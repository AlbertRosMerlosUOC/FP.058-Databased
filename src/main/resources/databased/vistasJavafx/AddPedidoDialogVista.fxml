<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="databased.vistasJavafx.AddPedidoDialogVistaController">
   <children>
      <VBox layoutX="30.0" layoutY="37.0" prefHeight="367.0" prefWidth="300.0" spacing="10.0" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Label text="Codigo" />
            <TextField fx:id="tCodigo" />
            <Label text="Descripción" />
            <TextField fx:id="tDescripcion" />
            <Label fx:id="tPVP" text=" Precio de venta" />
            <TextField fx:id="tPrecioVenta" />
            <Label text="Gastos de envío" />
            <TextField fx:id="tGastosEnvio" />
            <Label text="Tiempo de preparación (min)" />
            <TextField fx:id="tTiempoPreparacion" />
            <ButtonBar prefHeight="40.0" prefWidth="200.0">
              <buttons>
                <Button fx:id="btnAddArticulo" mnemonicParsing="false" onAction="#addPedido" text="Añadir" />
                  <Button fx:id="btnCancelAddArticulo" mnemonicParsing="false" onAction="#cancelAddPedido" text="Cancelar" />
              </buttons>
            </ButtonBar>
         </children>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
